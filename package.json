{
	"name": "tilde",
	"displayName": "Tilde Syntax Highlighter",
	"description": "Syntax highlighter on Tilde files",
	"version": "1.0.2",
	"author": "Auracle",
	"repository": "https://github.com/AuracleTech/Tilde-vscode",
	"publisher": "Auracle",
	"icon": "img/icon.png",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "tilde",
				"aliases": [
					"ti",
					"tilde"
				],
				"extensions": [
					"tilde",
					"ti"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "tilde",
				"scopeName": "source.tilde",
				"path": "./syntaxes/tilde.tmLanguage.json"
			}
		]
	}
}
